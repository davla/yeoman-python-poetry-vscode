{
  "name": "generator-python-poetry-vscode",
  "scripts": {
    "changelog": "node ./scripts/changelog.js",
    "format": "eslint . --ext js --fix --fix-type layout | dev-null",
    "lint": "eslint . --ext js",
    "lint:fix": "npm run lint -- --fix",
    "test": "npm run test:unit",
    "test:cmd": "mocha",
    "test:end-to-end": "npm run test:cmd -- 'test/end-to-end/**/*.js'",
    "test:end-to-end:ci": "npm run test:end-to-end -- --reporter json --reporter-option",
    "test:unit": "npm run test:cmd -- 'test/unit/**'",
    "test:unit:ci": "npm run test:unit -- --reporter json --reporter-option",
    "test:update-snapshots": "cross-env UPDATE_SNAPSHOT=1 npm run test:end-to-end",
    "semantic-release": "semantic-release"
  },
  "dependencies": {
    "@iarna/toml": "^2.2.5",
    "chalk": "^5.2.0",
    "email-validator": "^2.0.4",
    "generator-editorconf": "^0.3.1",
    "generator-gi": "^0.0.2",
    "generator-license": "^5.4.0",
    "giturl": "^1.0.1",
    "lodash": "^4.17.21",
    "remote-origin-url": "^2.0.0",
    "semver": "^7.3.8",
    "yeoman-generator": "^5.7.0",
    "yosay": "^2.0.2"
  },
  "devDependencies": {
    "@semantic-release/exec": "^6.0.3",
    "@types/chai": "^4.3.4",
    "@types/chai-as-promised": "^7.1.5",
    "@types/chai-subset": "^1.3.3",
    "@types/docopt": "^0.6.33",
    "@types/execa": "^2.0.0",
    "@types/lodash": "^4.14.191",
    "@types/mocha": "^10.0.1",
    "@types/semver": "^7.3.13",
    "@types/sinon": "^10.0.13",
    "@types/sinon-chai": "^3.2.9",
    "@types/yeoman-generator": "^5.2.11",
    "@types/yeoman-test": "^4.0.3",
    "chai": "^4.3.7",
    "chai-as-promised": "^7.1.1",
    "chai-subset": "^1.6.0",
    "conventional-changelog-conventionalcommits": "^5.0.0",
    "cross-env": "^7.0.3",
    "date-format": "^4.0.14",
    "dev-null-cli": "^2.0.0",
    "docopt": "^0.6.2",
    "eslint": "^8.30.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-xo": "^0.43.1",
    "eslint-plugin-chai-friendly": "^0.7.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-unicorn": "^47.0.0",
    "execa": "^7.1.1",
    "fast-glob": "^3.2.12",
    "mocha": "^10.2.0",
    "mocha-chai-jest-snapshot": "^1.1.4",
    "prettier": "^2.8.1",
    "semantic-release": "^21.0.3",
    "sinon": "^15.0.1",
    "sinon-chai": "^3.7.0",
    "yeoman-test": "^7.1.0",
    "yo": "^4.3.1"
  },
  "type": "module",
  "files": [
    "generators",
    "lib"
  ],
  "main": "generators/index.js",
  "engines": {
    "node": ">= 16.0.0",
    "npm": ">= 4.0.0"
  },
  "description": "Yeoman generator for Python packages, using Poetry and integrated with Visual Studio Code",
  "homepage": "https://github.com/davla/yeoman-python-poetry-vscode",
  "license": "GPL-3.0",
  "repository": "git@github.com:davla/yeoman-python-poetry-vscode.git",
  "version": "0.0.0-development",
  "author": {
    "name": "Davide Laezza",
    "email": "truzzialrogo@gmx.com",
    "url": "https://github.com/davla"
  },
  "keywords": [
    "python",
    "poetry",
    "vscode",
    "yeoman-generator"
  ]
}
